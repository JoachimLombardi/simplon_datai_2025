--
-- Table structure for table `recommendations_risks`
--

DROP TABLE IF EXISTS `recommendations_risks`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `recommendations_risks` (
  `recommendation_id` bigint(20) unsigned NOT NULL,
  `risk_id` bigint(20) unsigned NOT NULL,
  UNIQUE KEY `recommendations_risks_recommendation_id_risk_id_unique` (`recommendation_id`,`risk_id`),
  KEY `recommendations_risks_risk_id_foreign` (`risk_id`),
  CONSTRAINT `recommendations_risks_recommendation_id_foreign` FOREIGN KEY (`recommendation_id`) REFERENCES `recommendations` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `recommendations_risks_risk_id_foreign` FOREIGN KEY (`risk_id`) REFERENCES `risks` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
/*!40101 SET character_set_client = @saved_cs_client */;

