  CONSTRAINT `food_risks_food_id_foreign` FOREIGN KEY (`food_id`) REFERENCES `food` (`code`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `food_risks_risk_id_foreign` FOREIGN KEY (`risk_id`) REFERENCES `risks` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
