  `review` text COLLATE utf8mb4_unicode_ci,
  `nb_portions` int(11) NOT NULL,
  `side_food` text COLLATE utf8mb4_unicode_ci,
  `steps` text COLLATE utf8mb4_unicode_ci NOT NULL,
  `food` text COLLATE utf8mb4_unicode_ci NOT NULL,
  `archived` tinyint(1) NOT NULL,
  `published_at` date NOT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `recipes_name_unique` (`name`),
  FULLTEXT KEY `fti_recipes_food_steps` (`food`,`steps`)
